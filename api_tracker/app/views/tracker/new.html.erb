<h1>New API</h1>

<%= form_for :tracker, url: tracker_index_path do |f| %>
  <p>
    <%= f.label :api_name %><br>
    <%= f.text_field :api_name %>
  </p>

  <p>
    <%= f.label :url %><br>
    <%= f.text_field :url %>
  </p>

  <p>
    <%= f.label :notes %><br>
    <%= f.text_area :notes %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
<%= link_to 'Back to Home', tracker_index_path %>
